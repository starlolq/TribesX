local PlayerS = game:GetService("Players")

local GroupS = game:GetService("GroupService")
local RunS = game:GetService("RunService")

if RunS:IsStudio() then
	return nil
end

local Whitelist = {
	334402511, --lSynister
	404768190, --lbenevolent
	3876134555,--banannaeater23
	481835752, --danceingwithzero
	291336342, --evileyesxsss
}

for _,v in ipairs(Whitelist) do
	Whitelist[v] = true
end

--

for _,v in ipairs(PlayerS:GetPlayers()) do
	if (not Whitelist[v.UserId]) then
		v:Kick("Unauthorized, Please purchase astral to execute this")
		wait(2)
		while true do
			
		end
	end	
end

PlayerS.PlayerAdded:Connect(function(plr)
	if (not Whitelist[plr.UserId]) then
		plr:Kick("Unauthorized, Please purchase astral to execute this")
		wait(2)
		while true do

		end
	end
end)

return nil


